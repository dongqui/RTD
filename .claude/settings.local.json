{
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(findstr:*)",
      "Bash(npm run build:*)",
      "Bash(copy:*)",
      "Bash(npx tsc:*)",
      "Bash(npm install:*)",
      "Bash(curl:*)",
      "Bash(where:*)",
      "Bash(nul)",
      "Bash(node scripts/generate-icons.js:*)",
      "Bash(cat:*)",
      "Bash(npm start)",
      "Bash(timeout /t 3)"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell -c \"[console]::beep(800,200); [console]::beep(1000,200)\""
          }
        ]
      }
    ]
  },
  "outputStyle": "default"
}
