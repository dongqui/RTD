{
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(findstr:*)",
      "Bash(npm run build:*)",
      "Bash(copy:*)"
    ]
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "powershell -c \"[console]::beep(800,200); [console]::beep(1000,200)\""
          }
        ]
      }
    ]
  },
  "outputStyle": "default"
}
